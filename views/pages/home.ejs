<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Include the head partial for common HTML head elements -->
  <%- include("../partials/head"); %>
</head>
<body>
  <header>
    <nav class="navbar">
      <!-- Logo image -->
      <img src="Custom Logo.jpg" alt="Logo" class="logo">
      <ul>
        <!-- Navigation links -->
        <li><a href="/">Home</a></li>
        <li><a href="/about">About</a></li>
        <li><a href="/projects">Projects</a></li>
        <li><a href="/services">Services</a></li>
        <li><a href="/contact">Contact</a></li>
      </ul>
    </nav>
  </header>
  
  <main>
    <section class="content">
      <h1><%= title %></h1>
    
      <!-- Iterate through details and display each one -->
      <% details.forEach(detail => { %>
        <br>
        <%= detail %>
      <% }) %>

    </section>
  </main>
</body>
</html>
