<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Include the head partial for common HTML head elements -->
    <link rel="stylesheet" href="/stylesheets/style.css" type="text/css">
</head>

<body>
    <%- include("../partials/header"); %>
    <main>
        <section class="content">
            <h1><%= title %></h1>
            <p>These are some of the services I provide:</p>

            <ol>
                <!-- Iterate through the services and display each one -->
                <% services.forEach(service => { %>
                <li>
                    <h2><%= service.name %></h2>
                    <p><%= service.description %></p>
                </li>
                <% }) %>
            </ol>

        </section>
    </main>
    <%- include("../partials/footer"); %>

</body>

</html>
