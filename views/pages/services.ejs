<!DOCTYPE html>
<html lang="en">
<head>
  <%- include("../partials/head"); %>
</head>
<body>
  <center>
    <h1><%= title %></h1>
  </center>

  <ul class="service-list">
    <% services.forEach(service => { %>
      <li>
        <h2><%= service.name %></h2>
        <p><%= service.description %></p>
      </li>
    <% }) %>
  </ul>

  <center>
    <a href="/">Home</a>
  </center>
</body>
</html>
